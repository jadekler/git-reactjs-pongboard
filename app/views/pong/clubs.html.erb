<div class="ui center aligned container">
  <% @clubs.each do |club| %>
  <div class="club">
    <a class="ui blue image label" href="/<%= club[:resource_name] %>/leaderboard">
      <i class="<%= club[:country] %> flag"></i>
      <%= club[:name] %>
      <div class="detail"><%= club[:player_count] %></div>
    </a>
  </div>
  <% end %>
</div>
